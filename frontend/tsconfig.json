{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "node"
    ], // ðŸ‘ˆ thÃªm dÃ²ng nÃ y
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "types": [
        "src/types/index.ts"
      ],
      "types/*": [
        "src/types/*"
      ]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "postcss.config.cjs"
  ],
  "exclude": [
    "node_modules"
  ]
}